<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="avalonia_test.Views.LiveDatasource">
    <Grid ColumnDefinitions="Auto,5,*" RowDefinitions="Auto,Auto,Auto,Auto,Auto,*" Margin="5">
        <Label Grid.Row="0" Grid.Column="0">Host</Label>
        <TextBox Grid.Row="0" Grid.Column="2"></TextBox>

        <Label Grid.Row="1" Grid.Column="0">Username</Label>
        <TextBox Grid.Row="1" Grid.Column="2"></TextBox>

        <Label Grid.Row="2" Grid.Column="0">Password</Label>
        <TextBox Grid.Row="2" Grid.Column="2"></TextBox>

        <Label Grid.Row="3" Grid.Column="0">Database</Label>
        <TextBox Grid.Row="3" Grid.Column="2"></TextBox>
    </Grid>
</UserControl>
